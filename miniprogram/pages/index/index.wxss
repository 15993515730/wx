}
.slide-shade {
	position: absolute;
	bottom: 0 !important;
	width: 100%;
	height: 200rpx;
	background: linear-gradient(
	  270deg,
	  rgba(0, 0, 0, 0) 0%,
	  rgba(0, 0, 0, 0.8) 100%
	);
	background: -webkit-linear-gradient(
	  270deg,
	  rgba(0, 0, 0, 0) 0%,
	  rgba(0, 0, 0, 0.8) 100%
	);
	/* background: -moz-linear-gradient(270deg,rgba(0,0,0,0) 0%,rgba(0,0,0,0.8) 100%); */
	position: absolute;
	bottom: 0;
	left: 0;
	z-index: 9;
	line-height: 250rpx;
	text-align: left;
	overflow: hidden;
	border-radius: 0 0 8rpx 8rpx;
  }
  .swiper_word {
	width: calc(100% - 64rpx);
	width: -webkit-calc(100% - 64rpx);
	width: -moz-calc(100% - 64rpx);
	padding: 0 32rpx;
	display: inline-block;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
	color: #fff;
	}
